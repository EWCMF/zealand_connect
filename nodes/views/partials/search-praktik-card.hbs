{{#each json}}
<div class="card mb-3">
    <div class="row">
        <div class="col-auto">
            <img class="center-cropped" src="https://www.shopinimizaj.com/frontend/web/images/no_images.png">
        </div>
        <div class="col">
            <div class="card-body">
                <h5 class="card-title">{{title}}</h5>
                <h6 class="card-title">{{email}}</h6>
                <p class="card-text autobreak">{{post_text}}</p>

            </div>
        </div>
    </div>
    {{!-- <a href='/search-praktik/{{id}}' class="stretched-link"></a> --}}
    <div class="card-footer">
        <p>Start: {{post_start_date}} Slut: {{post_end_date}}</p>
    </div>
    <a href='/internship_view/{{id}}' class="stretched-link"></a>
</div>
{{/each}}