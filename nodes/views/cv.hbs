            {{/if}}
        </div>
    </div>
    <div class="row border rounded ml-1 mr-1 mb-4">
        <div class="col mb-2">
            <h4 class="mb-3 mt-2 d-flex align-self-center" data-key="Udlandsophold_og_frivilligt_arbejde">
                Udlandsophold og frivilligt arbejde   
            </h4>
            {{#if json.udenlandsophold_og_frivilligt_arbejde}}
            <p class="autobreak">
                {{json.udenlandsophold_og_frivilligt_arbejde}}
            </p>
            {{else}}
            <p class="autobreak">
                {{noData}}
            </p>
            {{/if}}
        </div>
    </div>
    <div class="row border rounded ml-1 mr-1 mb-4">
        <div class="col mb-2">
            <h4 class="mb-3 mt-2 d-flex align-self-center" data-key="Fritidsinteresser">
